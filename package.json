    "test:unit:watch": "jest --watch",
    "test:e2e": "npx playwright test",
    "test:e2e:ci": "start-server-and-test 'npm run start' http://localhost:3000 'npm run test:e2e'",
    "test:full": "npm run build && npm run test:unit && npm run lint && npm run format:check && npm run test:e2e:ci"
